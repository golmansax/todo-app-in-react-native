{
  "name": "TodoApp",
  "version": "0.0.1",
  "scripts": {
    "start-ios": "./node_modules/react-native/packager/packager.sh",
    "start-web": "nodemon web/index.js -e js,jsx,json",
    "test": "./node_modules/.bin/jest"
  },
  "jest": {
    "scriptPreprocessor": "jestSupport/scriptPreprocess.js",
    "setupEnvScriptFile": "jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map"
    ]
  },
  "devDependencies": {
    "babel-jest": "latest",
    "eslint": "*",
    "eslint-config-golmansax": "*",
    "jest-cli": "0.4.x",
    "travis-lint": "latest"
  },
  "dependencies": {
    "babel": "latest",
    "express": "latest",
    "react": "latest",
    "react-native": "latest",
    "react-native-button": "latest",
    "react-native-icons": "latest",
    "react-native-navbar": "latest"
  }
}
