{
  "name": "TodoApp",
  "version": "0.0.1",
  "scripts": {
    "build-web": "./node_modules/.bin/webpack --config web/webpack.config.js --progress --colors",
    "start-web": "node web/index.js",
    "test": "./node_modules/.bin/jest",
    "postinstall": "make link-modules",
    "watch-ios": "./node_modules/react-native/packager/packager.sh",
    "watch-web": "./node_modules/.bin/webpack-dev-server --config web/webpack.config.js --content-base web/public/build --progress --colors --hot --inline"
  },
  "jest": {
    "scriptPreprocessor": "jestSupport/scriptPreprocess.js",
    "setupEnvScriptFile": "jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map"
    ]
  },
  "devDependencies": {
    "babel-jest": "*",
    "babel-eslint": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-config-golmansax": "*",
    "eslint-plugin-react": "*",
    "http-proxy": "*",
    "jest-cli": "0.4.x",
    "react-hot-loader": "*",
    "travis-lint": "*",
    "webpack-dev-server": "*"
  },
  "dependencies": {
    "babel": "*",
    "babel-loader": "*",
    "babel-runtime": "*",
    "bootstrap-styl": "*",
    "css-loader": "*",
    "express": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "jeet": "*",
    "nib": "*",
    "react": "*",
    "react-bootstrap": "*",
    "react-native": "*",
    "react-native-button": "*",
    "react-native-icons": "*",
    "react-native-navbar": "*",
    "react-router": "*",
    "style-loader": "*",
    "stylus-loader": "*",
    "stylus-type-utils": "*",
    "url-loader": "*",
    "webpack": "*"
  }
}
