{
  "name": "TodoApp",
  "version": "0.0.1",
  "scripts": {
    "build-web": "./node_modules/.bin/webpack --config web/webpack.config.js --progress --colors",
    "start-web": "nodemon web/index.js -e js,jsx,json --watch web --watch shared",
    "test": "./node_modules/.bin/jest",
    "watch-ios": "./node_modules/react-native/packager/packager.sh",
    "watch-web": "./node_modules/.bin/webpack-dev-server --config web/webpack.config.js --content-base web/public/build --progress --colors"
  },
  "jest": {
    "scriptPreprocessor": "jestSupport/scriptPreprocess.js",
    "setupEnvScriptFile": "jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map"
    ]
  },
  "devDependencies": {
    "babel-jest": "*",
    "eslint": "*",
    "eslint-config-golmansax": "*",
    "http-proxy": "*",
    "jest-cli": "0.4.x",
    "travis-lint": "*"
  },
  "dependencies": {
    "babel": "*",
    "babel-loader": "*",
    "babel-runtime": "*",
    "css-loader": "*",
    "express": "*",
    "react": "*",
    "react-native": "*",
    "react-native-button": "*",
    "react-native-icons": "*",
    "react-native-navbar": "*",
    "react-router": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  }
}
